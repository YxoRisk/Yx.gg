AutoShootButton.Size = UDim2.new(0, 120, 0, 30)
AutoShootButton.Font = Enum.Font.SourceSansSemibold
AutoShootButton.Text = "Auto Shoot Off"
AutoShootButton.TextColor3 = Color3.fromRGB(255, 255, 255)
AutoShootButton.TextScaled = true
AutoShootButton.TextSize = 14.000
AutoShootButton.TextWrapped = true

AutoShootButton.MouseButton1Click:Connect(function()
    autoShoot = not autoShoot
    if autoShoot then
        AutoShootButton.Text = "Auto Shoot On"
    else
        AutoShootButton.Text = "Auto Shoot Off"
    end
end)

UICorner_2.Parent = AutoShootButton

-- Function to ensure the frame is centered when dragged
local function TopContainer()
    Frame.Position = UDim2.new(0.5, -Frame.AbsoluteSize.X / 2, 0, -Frame.AbsoluteSize.Y / 2)
end

TopContainer()
Frame:GetPropertyChangedSignal("AbsoluteSize"):Connect(TopContainer)

